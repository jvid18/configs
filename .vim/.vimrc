set number relativenumber
set mouse=a
set numberwidth=1
set clipboard=unnamed
syntax enable
set showcmd
set ruler
set cursorline
set encoding=utf-8
set showmatch
set sw=2
set laststatus=2

" Plugins
source ~/configs/.vim/UltiSnips/plugins.vim

" Plugins config
source ~/configs/.vim/UltiSnips/plugin-config.vim

" Custom commands.
source ~/configs/.vim/UltiSnips/maps.vim
